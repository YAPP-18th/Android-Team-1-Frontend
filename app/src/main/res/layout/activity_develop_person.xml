<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    tools:context=".ui.setting.DevelopPersonActivity">


    <TextView
        android:id="@+id/tv_develop_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="만든 사람들"
        style="@style/TextAppearance.NotosanskrMediumStyle"
        android:textColor="@color/grey_dark2"
        android:textSize="18dp"
        app:layout_constraintVertical_bias="0.02"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

      <androidx.constraintlayout.widget.Guideline
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:id="@+id/guideline1"
          app:layout_constraintGuide_begin="16dp"
          android:orientation="vertical"/>

    <ImageView
        android:id="@+id/img_develop_back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/icons_arrow_left"
        app:layout_constraintStart_toStartOf="@id/guideline1"
        app:layout_constraintTop_toTopOf="@id/tv_develop_title"
        app:layout_constraintBottom_toBottomOf="@id/tv_develop_title"/>


    <ImageView
        android:id="@+id/img_develop_plan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/icons_creators_pink"
        android:layout_marginTop="44dp"
        app:layout_constraintTop_toBottomOf="@id/tv_develop_title"
        app:layout_constraintStart_toStartOf="@id/guideline1"/>


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/img_develop_plan"
        android:text="PLAN"
        style="@style/TextAppearance.ComicSansStyle"
        android:id="@+id/tv_develop_plan"
        app:layout_constraintStart_toEndOf="@id/img_develop_plan"
        android:layout_marginStart="16dp"
        android:textColor="@color/pink2fb"
        android:textSize="14dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/tv_develop_plan"
        app:layout_constraintStart_toEndOf="@id/img_develop_plan"
        android:layout_marginStart="16dp"
        android:text="최현정"
        android:textColor="@color/grey_dark2"
        android:textSize="14dp"
        android:layout_marginTop="4dp"
        style="@style/TextAppearance.NotosanskrRegularStyle"/>


    <ImageView
        android:id="@+id/img_develop_design"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/icons_creators_blue"
        android:layout_marginTop="60dp"
        app:layout_constraintTop_toBottomOf="@id/img_develop_plan"
        app:layout_constraintStart_toStartOf="@id/guideline1"/>


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/img_develop_design"
        android:text="DESIGN"
        style="@style/TextAppearance.ComicSansStyle"
        android:id="@+id/tv_develop_design"
        app:layout_constraintStart_toEndOf="@id/img_develop_design"
        android:layout_marginStart="16dp"
        android:textColor="@color/pink2fb"
        android:textSize="14dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/tv_develop_design"
        app:layout_constraintStart_toEndOf="@id/img_develop_design"
        android:layout_marginStart="16dp"
        android:text="김서정"
        android:textColor="@color/grey_dark2"
        android:textSize="14dp"
        android:layout_marginTop="4dp"
        style="@style/TextAppearance.NotosanskrRegularStyle"/>


    <ImageView
        android:id="@+id/img_develop_android"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/icons_creators_yellow"
        android:layout_marginTop="60dp"
        app:layout_constraintTop_toBottomOf="@id/img_develop_design"
        app:layout_constraintStart_toStartOf="@id/guideline1"/>


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style="@style/TextAppearance.ComicSansStyle"
        app:layout_constraintTop_toTopOf="@id/img_develop_android"
        android:text="FRONT-END"
        android:id="@+id/tv_develop_android"
        app:layout_constraintStart_toEndOf="@id/img_develop_android"
        android:layout_marginStart="16dp"
        android:textColor="@color/pink2fb"
        android:textSize="14dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/tv_develop_android"
        app:layout_constraintStart_toEndOf="@id/img_develop_android"
        android:layout_marginStart="16dp"
        android:text="김수현, 김유나, 한기호"
        android:textColor="@color/grey_dark2"
        android:textSize="14dp"
        android:layout_marginTop="4dp"
        style="@style/TextAppearance.NotosanskrRegularStyle"/>

    <ImageView
        android:id="@+id/img_develop_server"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/icons_creators_puple"
        android:layout_marginTop="60dp"
        app:layout_constraintTop_toBottomOf="@id/img_develop_android"
        app:layout_constraintStart_toStartOf="@id/guideline1"/>


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/img_develop_server"
        android:text="BACK-END"
        style="@style/TextAppearance.ComicSansStyle"
        android:id="@+id/tv_develop_server"
        app:layout_constraintStart_toEndOf="@id/img_develop_server"
        android:layout_marginStart="16dp"
        android:textColor="@color/pink2fb"
        android:textSize="14dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/tv_develop_server"
        app:layout_constraintStart_toEndOf="@id/img_develop_server"
        android:layout_marginStart="16dp"
        android:text="박현국, 조현우"
        android:textColor="@color/grey_dark2"
        android:textSize="14dp"
        android:layout_marginTop="4dp"
        style="@style/TextAppearance.NotosanskrRegularStyle"/>

</androidx.constraintlayout.widget.ConstraintLayout>